<%@ include file="/Frame/page/jspf/include/jsp_frame_lrud.jspf"%>
<script type="text/javascript">
	var mytoptd = {};
	$(function(){
		if(!isNaN(mytoptd.height))
			changeLayout(isLeftRight(),mytoptd.height/($(document.body).height()-mytoptd.height));
	});
	Layout.initRegionName("rightup", self.name, Layout.getRegionName("FirstFrame"));
	Layout.initRegionName("rightdown", self.name, Layout.getRegionName("SecondFrame"));
</script>